{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "common/services": ["src/common/win-services", "src/common/win-services/index"],
      "modules/components": ["src/modules/components", "src/modules/components/index"],
      "common/models": ["src/common/win-models/index"],
      "common/models/*": ["src/common/win-models/*"]
    },
    "lib": [
      "es2017",
      "dom"
    ]
  }
}